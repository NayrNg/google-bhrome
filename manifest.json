{
  "manifest_version": 2,
  "name": "Google Bhrome",
  "description": "Replace 'C's with 'B's",
  "version": "0.2.0",
  "homepage_url": "https://github.com/NayrNg/google-bhrome",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_end"
    }
  ]
}